services:
  <%_ props.values.forEach(function(v) { _%>
  - name: "<%- v.props.database -%>"
    path: "<%- v.props.targetDir -%>"
    description: "<%- v.props.version -%>"
    notes: "Check <%- v.target -%> for credentials and configuration"
  <%_ }); _%>

secrets:
- name: vault
  provider: vault
  config:
    binary: vault
    loginArgs: ["login", "-format=json"]
    endpoint:
  files:
  entries:

environment:
  - name: dev
    description: Run services locally